{
  "name": "@ipguk/react-ui",
  "version": "2.30.0",
  "description": "React UI component library for IPG web applications",
  "author": "IPG-Automotive-UK",
  "license": "MIT",
  "repository": "IPG-Automotive-UK/react-ui",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs --jsxFragment React.Fragment",
    "start": "microbundle-crl watch --no-compress --format modern,cjs --jsxFragment React.Fragment",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint ./src",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom --collectCoverage=true",
    "test:watch": "react-scripts test --env=jsdom",
    "storybook": "start-storybook -s static -p 6006",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/preset-react": "^7.17.12",
    "@storybook/addon-actions": "^6.5.4",
    "@storybook/addon-essentials": "^6.5.4",
    "@storybook/addon-interactions": "^6.5.4",
    "@storybook/addon-links": "^6.5.4",
    "@storybook/addon-storysource": "^6.5.4",
    "@storybook/builder-webpack5": "^6.5.4",
    "@storybook/manager-webpack5": "^6.5.4",
    "@storybook/node-logger": "^6.5.4",
    "@storybook/preset-create-react-app": "^4.1.0",
    "@storybook/react": "^6.5.4",
    "@storybook/testing-library": "^0.0.11",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^14.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-react": "^11.0.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-standard": "^5.0.0",
    "microbundle-crl": "^0.13.11",
    "mutationobserver-shim": "^0.3.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "resize-observer-polyfill": "^1.5.1",
    "storybook-dark-mode": "^1.1.2",
    "webpack": "^5.72.0"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.6.2",
    "@mui/material": "^5.6.4",
    "@mui/x-data-grid": "^5.10.0",
    "colord": "^2.9.2",
    "material-ui-popup-state": "^4.0.2",
    "react": "^18.1.0",
    "react-colorful": "^5.5.1",
    "react-dom": "^18.1.0",
    "react-hook-form": "^7.30.0",
    "use-debouncy": "^4.3.0",
    "zxcvbn": "^4.4.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/*.stories.{js,jsx}"
    ]
  }
}
