{
  "name": "@ipguk/react-ui",
  "version": "2.11.0",
  "description": "React UI component library for IPG web applications",
  "author": "IPG-Automotive-UK",
  "license": "MIT",
  "repository": "IPG-Automotive-UK/react-ui",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs --jsxFragment React.Fragment",
    "start": "microbundle-crl watch --no-compress --format modern,cjs --jsxFragment React.Fragment",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint ./src",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom --reporters=jest-html-reporter --collectCoverage=true",
    "test:watch": "react-scripts test --env=jsdom",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "react": "^17.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@storybook/addon-actions": "^6.4.0-beta.29",
    "@storybook/addon-essentials": "^6.4.0-beta.29",
    "@storybook/addon-links": "^6.4.0-beta.29",
    "@storybook/addon-storysource": "^6.4.0-beta.29",
    "@storybook/builder-webpack5": "^6.5.0-alpha.61",
    "@storybook/manager-webpack5": "^6.5.0-alpha.61",
    "@storybook/preset-create-react-app": "^4.1.0",
    "@storybook/react": "^6.4.0-beta.29",
    "@testing-library/dom": "^8.0.0",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.0.6",
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.3",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "jest-html-reporter": "^3.4.2",
    "microbundle-crl": "^0.13.11",
    "mutationobserver-shim": "^0.3.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-is": "^17.0.0",
    "react-scripts": "^5.0.0"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.5.1",
    "@mui/material": "^5.5.3",
    "@mui/x-data-grid": "^5.7.0",
    "colord": "^2.9.2",
    "material-ui-popup-state": "^2.0.0",
    "react-colorful": "^5.5.1",
    "react-hook-form": "^7.29.0",
    "use-debouncy": "^4.3.0",
    "zxcvbn": "^4.4.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/*.stories.{js,jsx}"
    ]
  }
}
