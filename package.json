{
  "name": "@ipguk/react-ui",
  "version": "4.0.2",
  "description": "React UI component library for IPG web applications",
  "author": "IPG-Automotive-UK",
  "license": "MIT",
  "repository": "IPG-Automotive-UK/react-ui",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs --jsxFragment React.Fragment",
    "build-storybook": "build-storybook",
    "prepare": "run-s build",
    "start": "microbundle-crl watch --no-compress --format modern,cjs --jsxFragment React.Fragment",
    "storybook": "start-storybook -s static -p 6006",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint ./src",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom --collectCoverage=true",
    "test:watch": "react-scripts test --env=jsdom",
    "type-check": "tsc --noEmit --skipLibCheck"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/preset-react": "^7.18.6",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/addon-storysource": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/node-logger": "^6.5.16",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/dom": "^9.3.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.2.1",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@types/react-text-mask": "^5.4.11",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "cross-env": "^7.0.3",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-react": "^13.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-standard": "^5.0.0",
    "microbundle-crl": "^0.13.11",
    "mutationobserver-shim": "^0.3.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "resize-observer-polyfill": "^1.5.1",
    "storybook-dark-mode": "^3.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^4.9.4",
    "webpack": "^5.83.1"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.1",
    "@mui/styles": "^5.13.1",
    "@mui/x-data-grid": "^6.4.0",
    "colord": "^2.9.3",
    "css-mediaquery": "^0.1.2",
    "material-ui-confirm": "^3.0.9",
    "material-ui-popup-state": "^5.0.8",
    "mui-file-dropzone": "^4.0.2",
    "react": "^18.2.0",
    "react-colorful": "^5.6.1",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-text-mask": "^5.5.0",
    "use-debouncy": "^4.4.0",
    "zxcvbn": "^4.4.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.stories.{js,jsx,ts,tsx}"
    ]
  }
}
